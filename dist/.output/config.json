{
  "version": 3,
  "framework": {
    "version": "0.66.3"
  },
  "routes": [
    {
      "src": "/remittance-api",
      "dest": "/remittance-api/recipients",
      "status": 301,
      "headers": {
        "Location": "/remittance-api/recipients"
      }
    },
    {
      "src": "/trade-api",
      "dest": "/trade-api/trades",
      "status": 301,
      "headers": {
        "Location": "/trade-api/trades"
      }
    },
    {
      "src": "/checkout-api",
      "dest": "/checkout-api/~endpoints",
      "status": 301,
      "headers": {
        "Location": "/checkout-api/~endpoints"
      }
    },
    {
      "src": "/wallet-api",
      "dest": "/wallet-api/balances",
      "status": 301,
      "headers": {
        "Location": "/wallet-api/balances"
      }
    }
  ]
}